	  <bind-xml name="fileCount" node="element"/>
	</field>
	
	<field name="container" type="string">
	  <bind-xml name="container" node="element"/>
	</field>
	
	<field name="mediaType" type="string">
	  <bind-xml name="mediaType" node="element"/>
	</field>
	
	
	<field name="extraInfoFieldNames" type="string" collection="arraylist" get-method="getExtraInfoFieldNames2" set-method="addExtraInfoFieldName">
	  <bind-xml name="FieldName" location="ExtraInfo" node="element"/>
	</field>


	<field name="extraInfoFieldValuesMap" collection="map">
	  <bind-xml name="extraInfoField" location="ExtraInfo" >
		<class name="org.exolab.castor.mapping.MapItem">
		  <field name="key" type="java.lang.String">
			<bind-xml node="attribute"/>
		  </field>
		  <field name="value" type="java.lang.String"/>
		</class>
	  </bind-xml>
	</field>

  </class>
  
  <class name="net.sf.xmm.moviemanager.models.ModelSeries">
	
	<map-to xml="ModelSeries" />
	
	<field name="ModelMovie" type="net.sf.xmm.moviemanager.models.ModelMovie" get-method="getMovie" set-method="setMovie">
	  <bind-xml name="ModelMovie"/>
	</field>
	
	<field name="episodes" type="net.sf.xmm.moviemanager.models.ModelEpisode" collection="arraylist">
	  <bind-xml name="ModelEpisode" />
	</field>
	
  </class>
  
  
  <class name="net.sf.xmm.moviemanager.models.ModelExportXML">
	
	<map-to xml="ModelExportXML" />
	
	<field name="movieManagerVersion" type="java.lang.String">
	  <bind-xml name="MovieManager-version" />
	</field>

	<field name="movies" type="net.sf.xmm.moviemanager.models.ModelMovie" collection="arraylist">
	  <bind-xml name="ModelMovie" />
	</field>
	
	<field name="series" type="net.sf.xmm.moviemanager.models.ModelSeries" collection="arraylist">
	  <bind-xml name="ModelSeries" />
	</field>
	
  </class>
  
