	</field>
	
  </class>
  
  
  <class name="net.sf.xmm.moviemanager.models.ModelAdditionalInfo">
	
	<field name="subtitles" type="string">
	  <bind-xml name="subtitles" node="element"/>
	</field> 
	
	<field name="duration" type="integer">
	  <bind-xml name="duration" node="element"/>
	</field> 
	
	<field name="fileSize" type="integer">
	  <bind-xml name="fileSize" node="element"/>
	</field> 
	
	<field name="cds" type="integer" get-method="getCDs">
	  <bind-xml name="cds" node="element"/>
	</field> 
	
	<field name="cdCases" type="double" get-method="getCDCases">
	  <bind-xml name="cdCases" node="element"/>
	</field> 
	
	<field name="resolution" type="string">
	  <bind-xml name="resolution" node="element"/>
	</field> 
	
	<field name="videoCodec" type="string">
	  <bind-xml name="videoCodec" node="element"/>
	</field> 
	
