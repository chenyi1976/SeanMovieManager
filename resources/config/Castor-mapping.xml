	</field>
	
	<field name="personalRating" type="string">
	  <bind-xml name="personalRating" location="GeneralInfo"/>
	</field>
	
	<field name="plot" type="string">
	  <bind-xml name="plot" location="GeneralInfo"/>
	</field>
	
	<field name="cast" type="string">
